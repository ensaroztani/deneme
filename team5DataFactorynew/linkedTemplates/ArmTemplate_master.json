						"value": "[parameters('factoryName')]"
					},
					"FileServerFourthCoffee_password": {
						"value": "[parameters('FileServerFourthCoffee_password')]"
					},
					"SqlServerVanArsdel_connectionString": {
						"value": "[parameters('SqlServerVanArsdel_connectionString')]"
					},
					"FileServerFourthCoffee_properties_typeProperties_host": {
						"value": "[parameters('FileServerFourthCoffee_properties_typeProperties_host')]"
					},
					"FileServerFourthCoffee_properties_typeProperties_userId": {
						"value": "[parameters('FileServerFourthCoffee_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'team5DataFactorynew_ArmTemplate_1')]"
			]
		}
	]
}